{
  "name": "CF BPGC Standings",
  "description": "This extension filters the standings for a given contest on Codeforces to show only active competitors from BPGC",
  "version": "1.0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["script.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": "icons/icon16.png",
    "default_popup": "index.html",
    "default_title": "CF BPGC Standings"
  },
  "permissions": ["tabs", "storage"]
}